package Collection;

import java.util.HashMap;
import java.util.Map;

public class Mapa {

	public static void main(String[] args) {
		
		Map<Integer, String> usuarios = new HashMap<>();
		
		usuarios.put(1, "Rafaela");
		usuarios.put(2,"João");
		usuarios.put(3, "Maycon");
		
		System.out.println(usuarios);
	}

}
