package Classe;

public class ConstrutorTeste {
	
	int dia ;
	int mes;
	int ano;
	
	ConstrutorTeste(){
		dia = 1;
		mes = 1;
		ano = 1970;
	}
	
	ConstrutorTeste(int diaInicial, int mesInicial, int anoInicial){
		dia = diaInicial;
		mes = mesInicial;
		ano = anoInicial;
	}
	
	String imprimirDataFormatada() {		
      return String.format("%d/%d/%d", dia, mes, ano);
	}
	
	
}
