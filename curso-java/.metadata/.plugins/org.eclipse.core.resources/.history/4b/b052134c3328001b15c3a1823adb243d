package Classe;

public class ConstrutorTeste {
	
	int dia;
	int mes;
	int ano;
	
	ConstrutorTeste(){
		imprimirNaTela();
	}
	
	ConstrutorTeste(){
		
	
	}
	
	void imprimirNaTela() {		
      System.out.println("1/1/1970");
	}
	
	void imprimirNaTela(int dia, int mes, int ano) {
		String.format("%d/%d/%d", dia, mes, ano);
	}
}
