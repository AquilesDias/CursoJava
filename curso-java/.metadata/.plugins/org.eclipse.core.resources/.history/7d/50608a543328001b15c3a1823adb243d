package Classe;

public class ConstrutorTeste {
	
	int dia;
	int mes;
	int ano;
	
	ConstrutorTeste(){
		imprimirNaTela();
	}
	
	ConstrutorTeste(int dia, int mes, int ano){
		imprimirNaTela2();
	
	}
	
	void imprimirNaTela() {		
      System.out.println("1/1/1970");
	}
	
	void imprimirNaTela2(int dia, int mes, int ano) {
		String.format("%d/%d/%d", dia, mes, ano);
	}
}
