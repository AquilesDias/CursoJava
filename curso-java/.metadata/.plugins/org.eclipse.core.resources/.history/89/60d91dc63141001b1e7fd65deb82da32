package excecao;

import java.util.Scanner;

public class Finally {

    public static void main(String[] args) {

	try {

	    Scanner sc = new Scanner(System.in);

	    System.out.println(7 / sc.nextInt());

	    sc.close();

	} catch (Exception e) {
	    System.out.println(e.getMessage());
	}

	System.out.println("FIM!");

    }
}
