package Classe;

public class DesafioPessoa {
	
	String nomePessoa;
	double pesoPessoa;
	
	DesafioComida dc = new DesafioComida();
	
	DesafioPessoa(){}
	
	DesafioPessoa(String nome, double pesoPessoa){
		this.nomePessoa = nome;
		this.pesoPessoa = pesoPessoa;		
	}
	
	double comer(double comida) {
		return pesoPessoa + dc.pesoComida;
	}

}
