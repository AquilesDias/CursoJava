package Classe;

public class ConstrutorTeste {
	
	int dia;
	int mes;
	int ano;
	
	ConstrutorTeste(){
		imprimirNaTela();
	}
	
	ConstrutorTeste(int dia, int mes, int ano){
		dia = dia;
		mes = mes;
		ano = ano;
	}
	
	void imprimirDataFormatada() {		
      String.format("%d/%d/%d", dia, mes, ano);
	}
	
}
