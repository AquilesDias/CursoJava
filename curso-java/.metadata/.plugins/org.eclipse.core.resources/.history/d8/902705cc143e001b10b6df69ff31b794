package streams;

public class ProdutoFilter {

    final String nome;
    final double preco;
    final int desconto;
    boolean freteGratis= false;
    
    ProdutoFilter(String nome, double preco, int desconto, boolean freteGratis){
	this.nome = nome;
	this.preco = preco;
	this.desconto = desconto;
	this.freteGratis = freteGratis;
    }
}
