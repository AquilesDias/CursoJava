package oo.composicao;

public class Carro {
	
	Motor motor = new Motor();
	
	void acelerar() {
		motor.fatorInjecao += 0.4;
	}
	
	void freiar() {
		motor.fatorInjecao -= 0.4;
	}
	
	void ligado() {
		motor.ligado = true;
	}
	
	void desligado() {
		motor.ligado = false;
	}
	
	void estaLigad() {
		return motor.ligado;
	}

}
