package streams;

public class ProdutoFilter {

    final String nome;
    final double preco;
    final int desconto;
    final double freteGratis;

    ProdutoFilter(String nome, double preco, int desconto, double freteGratis) {
	this.nome = nome;
	this.preco = preco;
	this.desconto = desconto;
	this.freteGratis = freteGratis;
    }
}
