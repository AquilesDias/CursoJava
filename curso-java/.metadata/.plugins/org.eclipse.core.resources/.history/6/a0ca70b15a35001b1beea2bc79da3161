package oo.heranca;

public class Jogo {
	public static void main(String[] args) {

		Jogador j1 = new Jogador();
		Jogador j2 = new Jogador();

		j1.x = 10;
		j1.y = 10;

		j2.x = 10;
		j2.y = 10;

		System.out.println("Quantidade de vida: " + j1.vida);
		System.out.println("Quantidade de vida: " + j2.vida);

		System.out.println(j1.y);

	}
}
